#include<iostream>
using namespace std;
int main()
{
    int arr[10], i, num, index = 0;
    cout<<"Enter 10 Numbers: ";
    for(i=0; i<10; i++)
        cin>>arr[i];
    cout<<"\nEnter a Number to Search: ";
    cin>>num;
    if(num <=0 ){
        cout<<"enter a valid number :";
    }else{
    for(i=0; i<10; i++)
    {
        if(arr[i]==num)
        {
            index = i;
            break;
        }
    }
    }
    
    if(index != 0){
    cout<<"\nFound at Index No."<<index;
    cout<<endl;
    }
    return 0;
}
