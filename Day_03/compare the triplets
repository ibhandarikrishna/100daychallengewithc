#include<iostream>
using namespace std;
int main(){
    int a[3];
    int b[3];
    int alise=0;
    int bob = 0;
   
    for(int i=0;i<3;i++){
        cin>>a[i];

    }
     for(int i=0;i<3;i++){
        cin>>b[i];

    }
     for(int i=0;i<3;i++){
     if (a[i]>b[i]) {
         alise++;
     }else if (a[i]<b[i]) {
     bob++;
     }else {
     
     }
    }
    
    cout<<alise<<" "<<bob;
}
