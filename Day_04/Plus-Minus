#include <iostream>
using namespace std;
int main(){
    int n;
    double pos = 0;
    double neg = 0;
    double z = 0;
    cin>>n;
    float arr[n];
    for(int i=0; i<n;i++) {
    cin>>arr[i];
    }
    for (int i=0; i<n;i++) {
    if(arr[i] > 0){
        pos+=1;
    }else if (arr[i]<0) {
      neg++;
    }else if (arr[i] == 0) {
    z++;
    }else {
    
    }
    }
    double x = pos/n;
    double y = neg/n;
    double r = z/n;
    cout<<x<<endl;
    cout<<y<<endl;
    cout<<r<<endl;
    
}
